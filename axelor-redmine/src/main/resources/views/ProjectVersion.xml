<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

	<form id="redmine-project-version-form" name="project-version-form"
		title="Project.version" model="com.axelor.apps.businesssupport.db.ProjectVersion"
		extension="true">

		<extend target="//panel-dashlet[@name='taskDashletPanel']">
			<attribute name="canSearch" value="true" />
			<insert position="before">
				<field name="totalProgress" readonly="true" widget="SelectProgress"
					colSpan="12" />
			</insert>
		</extend>

	</form>

</object-views>
