<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<form name="project-form" title="Project" id="redmine-project-form"
		model="com.axelor.apps.project.db.Project" onLoad="action-project-on-load-group"
		onNew="action-project-group-on-new-project" extension="true">

		<extend target="//panel[@name='contractPanel']">
			<insert position="after">
				<panel name="redminePanel" title="Redmine">
					<field name="redmineId" />
				</panel>
			</insert>
		</extend>
		
		<extend target="//spacer[@name='invoicingSequenceSelectSpacer']">
			<insert position="before">
				<field name="consolidatePhaseWhenInvoicing" colSpan="4" showIf="projectTypeSelect == 1"/>
			</insert>
			<attribute name="colSpan" value="3"/>
		</extend>

	</form>

</object-views>
