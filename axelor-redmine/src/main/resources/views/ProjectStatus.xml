<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	
	<form id="redmine-project-status-form" name="project-status-form" title="Status" model="com.axelor.apps.project.db.ProjectStatus" extension="true">
		<extend target="/">
			<attribute name="onSave" value="action-project-status-method-update-project-version-progress"/>
		</extend>
	</form>
	
	<action-method name="action-project-status-method-update-project-version-progress">
		<call class="com.axelor.apps.redmine.web.RedmineProjectStatusController" method="updateProjectVersionProgress"/>
	</action-method>
	
</object-views>