<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="base" package="com.axelor.apps.base.db" />

	<entity name="AppRedmine" lang="java" cachable="true" extends="App">

		<!-- REDMINE CONFIGURATION DETAILS -->
		<string name="uri" title="URI" />
		<string name="apiAccessKey" title="Api Access Key" />
		<many-to-one name="company" ref="com.axelor.apps.base.db.Company"
			title="Company" required="true" />

		<!-- REDMINE CUSTOM FIELD CONFIGURATION DETAILS -->
		<string name="redmineProjectInvoiceable" required="true" />
		<string name="redmineProjectClientPartner" required="true" />
		<string name="redmineProjectInvoicingSequenceSelect" required="true" />
		<string name="redmineIssueProduct" required="true" />
		<string name="redmineIssueIsTaskRefused" required="true" />
		<string name="redmineIssueTaskDate" required="true" />
		<string name="redmineIssueEstimatedTime" required="true" />
		<string name="redmineIssueInvoiced" required="true" />
		<string name="redmineIssueAccountedForMaintenance" required="true" />
		<string name="redmineTimeSpentProduct" required="true" />
		<string name="redmineTimeSpentDurationForCustomer" required="true" />

		<!-- REDMINE CUSTOM FIELD CONFIGURATION DEFAULT VALUES -->
		<string name="redmineProjectInvoiceableDefault" />
		<string name="redmineProjectClientPartnerDefault" />
		<string name="redmineProjectInvoicingSequenceSelectDefault" />
		<string name="redmineIssueProductDefault" />
		<string name="redmineIssueIsTaskRefusedDefault" />
		<string name="redmineIssueTaskDateDefault" />
		<string name="redmineIssueEstimatedTimeDefault" />
		<string name="redmineIssueInvoicedDefault" />
		<string name="redmineIssueAccountedForMaintenanceDefault" />
		<string name="redmineTimeSpentProductDefault" />
		<string name="redmineTimeSpentDurationForCustomerDefault" />

	</entity>

</domain-models>
