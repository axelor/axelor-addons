<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">
  
  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="AppRossum" cacheable="true" lang="java">
  	
  	<one-to-one ref="com.axelor.apps.base.db.App" name="app"/>
  
    <string name="username" title="User name"/>
    <string name="password" title="Password"/>
    <string name="token" title="Token" help="It lasts upto 162 hours unless user logouts"/>
    <datetime name="tokenDateTime" title="Token created on"/>
    <boolean name="isValid" title="Valid connection"/>
    <boolean name="isInvoiceLineConsolidated" title="Consolidate invoice lines"/>
  </entity>  

</domain-models>
