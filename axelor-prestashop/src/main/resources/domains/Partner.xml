<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

	<module name="base" package="com.axelor.apps.base.db"/>

	<entity name="Partner" lang="java">
		<integer name="prestaShopId" nullable="true"/>
		<integer name="prestaShopVersion" nullable="true"/>
		<integer name="emailAddressPrestaShopVersion" nullable="true"/>

		<datetime name="prestaShopUpdateDateTime" nullable="true"/>

		<unique-constraint columns="prestaShopId"/>

		<finder-method name="findByPrestaShopId" using="prestaShopId"/>
		<finder-method name="findByRegistrationCode" using="registrationCode"/>
	</entity>

</domain-models>
