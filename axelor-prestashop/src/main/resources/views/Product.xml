<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

	<form name="product-form" title="Product" model="com.axelor.apps.base.db.Product" id="prestashop-product-form"
	onNew="action-group-base-product-onnew"
	onLoad="action-group-product-onload" width="large">
 		<panel-include view="product-form" from="axelor-base"/>
 		<panel name="prestashopPanel" title="Prestashop">
 			<field name="productSynchronizedInPrestashop" widget="boolean-switch" colSpan="12"/>
 		</panel>
	</form>
	
</object-views>
