<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ManufOrder" lang="java">
    <integer name="type" title="Type" selection="maintenance.type.select"/>

    <many-to-one name="machineType" default="1" ref="com.axelor.apps.production.db.MachineType" title="Machine type"/>
    <many-to-one name="machine" ref="com.axelor.apps.production.db.Machine" title="Machine"/>
  </entity>
  
</domain-models>
