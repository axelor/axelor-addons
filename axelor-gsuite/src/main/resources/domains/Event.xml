<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

	<module name="crm" package="com.axelor.apps.crm.db" />

	<entity name="Event" lang="java" extends="com.axelor.apps.base.db.ICalendarEvent">

		<one-to-many name="eventGoogleAccounts" ref="com.axelor.apps.gsuite.db.EventGoogleAccount" mappedBy="event" />
		<one-to-many name="taskGoogleAccounts" ref="com.axelor.apps.gsuite.db.TaskGoogleAccount" mappedBy="task"/>
		<string name="googleTaskId"/>
		<string name="tasklistName" title="Tasklist name"/>
		
		<finder-method name="findByGoogleTaskId" using="googleTaskId"/>
		
	</entity>

</domain-models>
