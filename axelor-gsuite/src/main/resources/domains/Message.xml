<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">
  
  <module name="message" package="com.axelor.apps.message.db"/>
  
  <entity name="Message" lang="java">
    
    <string name="messageId" title="Global message id" copy="false"/>
    
  	<many-to-many name="emailAccountSet"  ref="com.axelor.apps.message.db.EmailAccount" title="Gsuite accounts"/>
	  
	<finder-method name="findByMessageId" using="messageId" />
	  
  </entity>
  
</domain-models>
