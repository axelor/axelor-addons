<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

	<module name="gsuite" package="com.axelor.apps.gsuite.db" />

	<entity name="TaskGoogleAccount" cacheable="true">
	
		<many-to-one name="task" ref="com.axelor.apps.crm.db.Event"/>
		<string name="googleTaskId"/>
		<string name="googleTasklistId"/>
		<many-to-one name="googleAccount" ref="com.axelor.apps.gsuite.db.GoogleAccount"/>

		<finder-method name="findByGoogleTaskId" using="googleTaskId"/>
	
	</entity>

</domain-models>
