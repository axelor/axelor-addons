<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="docusign" package="com.axelor.apps.docusign.db"/>

  <entity name="DocuSignAccount" lang="java">

    <string name="name" title="Name"/>
    <string name="userId" title="User id"/>
    <string name="integrationKey" title="Integration Key"/>
    <string name="rsaKey" title="RSA key" large="true"/>
    <string name="baseURI" title="Base uri"/>
    <string name="oAuthBasePath" title="OAuth base path" help="Do not include https://"/>

  </entity>


</domain-models>