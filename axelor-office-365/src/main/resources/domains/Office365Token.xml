<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="office" package="com.axelor.apps.office.db" />

	<entity name="Office365Token" lang="java">

		<string name="token" readonly="true" large="true" copy="false"/>
		<integer name="folderTypeSelect" selection="office365.folder.type.select"/>
		<datetime name="fetchOn"/>

		<extra-code>
			<![CDATA[
				public static final int FOLDER_TYPE_CONTACT = 1;
				public static final int FOLDER_TYPE_CALENDAR = 2;
				public static final int FOLDER_TYPE_MAIL = 3;
			]]>
 		</extra-code>

	</entity>

</domain-models>