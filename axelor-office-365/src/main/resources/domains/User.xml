<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="auth" package="com.axelor.auth.db" />

	<entity name="User" sequential="true" lang="java">

		<many-to-one name="emailAccount" ref="com.axelor.apps.message.db.EmailAccount" title="Default office account" />
		<one-to-many name="emailAccountList" ref="com.axelor.apps.message.db.EmailAccount" title="Office accounts" mappedBy="ownerUser"/>

	</entity>

</domain-models>