<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="office" package="com.axelor.apps.office.db" />

	<entity name="ContactFolder" lang="java">

		<string name="office365Id" title="Office365 id" copy="false"/>
		<string name="contactDeltaToken" large="true" copy="false"/>
		<string name="parentFolderId" title="Parent folder" />
		<string name="name" />

		<many-to-one name="emailAccount" ref="com.axelor.apps.message.db.EmailAccount" title="Office account"/>
		<one-to-many name="tokenList" ref="com.axelor.apps.office.db.Office365Token" title="Tokens"/>

		<finder-method name="findByOffice365Id" using="office365Id"/>
		<finder-method name="findByName" using="name,emailAccount"/>

	</entity>

</domain-models>