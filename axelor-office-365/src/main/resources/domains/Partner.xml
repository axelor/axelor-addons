<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="base" package="com.axelor.apps.base.db" />

	<entity name="Partner" lang="java">

		<string name="office365Id" title="Office365 id" copy="false"/>
		<string name="nickName" title="Nick name"/>
		<string name="jobTitle" title="Job title"/>
		<date name="birthdate" title="Birth date" />

		<many-to-one name="emailAccount" ref="com.axelor.apps.message.db.EmailAccount" title="Office account"/>
		<many-to-one name="contactFolder" ref="com.axelor.apps.office.db.ContactFolder" title="Contact folder"/>

		<finder-method name="findByOffice365Id" using="office365Id"/>

		<entity-listener class="com.axelor.apps.office365.db.repo.Office365PartnerListener"/>

	</entity>

</domain-models>