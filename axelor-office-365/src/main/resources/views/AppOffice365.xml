<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<form name="app-office365-config-form" title="App office365" model="com.axelor.apps.base.db.AppOffice365" 
		canDelete="false" canNew="false" width="large">
		<panel name="mainPanel">
			<field name="clientId" required="true"/>
			<field name="redirectUri" required="true"/>
			<field name="clientSecret" required="true" widget="password"/>
			<field name="manageMessageRelatedTo"/>
		</panel>
	</form>

</object-views>